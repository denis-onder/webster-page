    "url": "https://github.com/DennisOnder/webster-page/issues"
  },
  "homepage": "https://github.com/DennisOnder/webster-page#readme",
  "devDependencies": {
    "browserify": "^16.2.3",
    "concurrently": "^4.1.0",
    "node-sass": "^7.0.0",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "zenscroll": "^4.0.2"
  }
